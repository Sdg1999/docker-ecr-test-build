pipeline{
  agent any
  stages{
    stage('Build'){
      steps{
        sh "chmod +x /var/lib/jenkins/jenkins-docker-ecr-push/docker_ci_build_ecr/docker_python/docker_ecr_push.sh"
        sh "./var/lib/jenkins/jenkins-docker-ecr-push/docker_ci_build_ecr/docker_python/docker_ecr_push.sh"
      }
    }
  }
  post {
        success {
            echo 'Pipeline succeeded!'
        }
        failure {
            echo 'Pipeline failed!'
        }
    }
}
